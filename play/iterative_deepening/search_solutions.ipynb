    "from minimax import minimax_decision\n",
    "\n",
    "def get_action(gameState, depth_limit):\n",
    "    # Turns out \"iterative deepening\" is just a for loop...\n",
    "    best_move = None\n",
    "    for depth in range(1, depth_limit+1):\n",
    "        best_move = minimax_decision(gameState, depth)\n",
    "    return best_move\n"
   ]
  }
